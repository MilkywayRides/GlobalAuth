{
  "name": "blazeneuro-developer",
  "version": "1.0.0",
  "description": "Your App Developer Portal - Authentication & API Management",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:prod": "node scripts/migrate-prod.js",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "preview": "npm run build && npm run start",
    "clean": "rm -rf .next out dist",
    "postinstall": "npm run db:generate",
    "dev:fast": "next dev --turbo --experimental-https",
    "test:load": "echo 'Load testing not configured'",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
